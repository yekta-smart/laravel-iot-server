{
	"name": "yekta-smart/laravel-iot-server",
	"license": "MIT",
	"require": {
		"php": "^8.1",
		"dnj/filesystem": "^1.0.1",
		"dnj/laravel-error-tracker-server": "^1.0.1",
		"dnj/laravel-user-logger": "^1.0.0"
	},
	"require-dev": {
		"phpunit/phpunit": "^9",
		"friendsofphp/php-cs-fixer": "^3.11",
		"orchestra/testbench": "^7.0",
		"dnj/tmp-filesystem": "^1.0.0"
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"autoload": {
		"psr-4": {
			"YektaSmart\\IotServer\\": "src/",
			"YektaSmart\\IotServer\\Database\\Factories\\": "database/factories/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"YektaSmart\\IotServer\\Tests\\": "tests/"
		}
	},
	"extra": {
		"laravel": {
			"providers": [
				"YektaSmart\\IotServer\\ServiceProvider"
			]
		}
	}
}
